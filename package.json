{
	"name": "Nodejs-skeleton",
	"version": "1.0.0",
	"main": "app.js",
	"scripts": {
		"server:start": "npx cross-env NODE_ENV=production node ./bin/www",
		"server:dev": "npx cross-env NODE_ENV=development node_modules/.bin/nodemon ./bin/www",
		"assets:dev": "npm run assets:development",
		"assets:development": "mix",
		"assets:watch": "mix watch",
		"assets:prod": "npm run assets:production",
		"assets:production": "mix --production"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DreamUpstream/ExpressJs-skeleton.git"
	},
