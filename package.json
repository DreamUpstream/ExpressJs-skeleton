	"bugs": {
		"url": "https://github.com/DreamUpstream/ExpressJs-skeleton/issues"
	},
	"homepage": "https://github.com/DreamUpstream/ExpressJs-skeleton#readme",
	"dependencies": {
		"axios": "^1.4.0",
		"bootstrap": "^5.3.0",
		"cookie-parser": "^1.4.6",
		"debug": "^4.3.4",
		"express": "^4.18.2",
		"fontawesome-free": "^1.0.4",
		"http-errors": "*",
		"morgan": "^1.10.0"
	},
	"devDependencies": {
		"laravel-mix": "^6.0.49",
		"nodemon": "^3.0.1",
